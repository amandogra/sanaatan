[
  {
    "id": "17001001",
    "book": "17",
    "chapter": "001",
    "verse": "001",
    "verse_text": " जनमेजय उवाच\r"
  },
  {
    "id": "17001001a",
    "book": "17",
    "chapter": "001",
    "verse": "001",
    "quarter_verse": "a",
    "verse_text": "एवं वृष्ण्यन्धककुले श्रुत्वा मौसलमाहवम्\r"
  },
  {
    "id": "17001001c",
    "book": "17",
    "chapter": "001",
    "verse": "001",
    "quarter_verse": "c",
    "verse_text": "पाण्डवाः किमकुर्वन्त तथा कृष्णे दिवं गते\r"
  },
  {
    "id": "17001002",
    "book": "17",
    "chapter": "001",
    "verse": "002",
    "verse_text": " वैशंपायन उवाच\r"
  },
  {
    "id": "17001002a",
    "book": "17",
    "chapter": "001",
    "verse": "002",
    "quarter_verse": "a",
    "verse_text": "श्रुत्वैव कौरवो राजा वृष्णीनां कदनं महत्\r"
  },
  {
    "id": "17001002c",
    "book": "17",
    "chapter": "001",
    "verse": "002",
    "quarter_verse": "c",
    "verse_text": "प्रस्थाने मतिमाधाय वाक्यमर्जुनमब्रवीत्\r"
  },
  {
    "id": "17001003a",
    "book": "17",
    "chapter": "001",
    "verse": "003",
    "quarter_verse": "a",
    "verse_text": "कालः पचति भूतानि सर्वाण्येव महामते\r"
  },
  {
    "id": "17001003c",
    "book": "17",
    "chapter": "001",
    "verse": "003",
    "quarter_verse": "c",
    "verse_text": "कर्मन्यासमहं मन्ये त्वमपि द्रष्टुमर्हसि\r"
  },
  {
    "id": "17001004a",
    "book": "17",
    "chapter": "001",
    "verse": "004",
    "quarter_verse": "a",
    "verse_text": "इत्युक्तः स तु कौन्तेयः कालः काल इति ब्रुवन्\r"
  },
  {
    "id": "17001004c",
    "book": "17",
    "chapter": "001",
    "verse": "004",
    "quarter_verse": "c",
    "verse_text": "अन्वपद्यत तद्वाक्यं भ्रातुर्ज्येष्ठस्य वीर्यवान्\r"
  },
  {
    "id": "17001005a",
    "book": "17",
    "chapter": "001",
    "verse": "005",
    "quarter_verse": "a",
    "verse_text": "अर्जुनस्य मतं ज्ञात्वा भीमसेनो यमौ तथा\r"
  },
  {
    "id": "17001005c",
    "book": "17",
    "chapter": "001",
    "verse": "005",
    "quarter_verse": "c",
    "verse_text": "अन्वपद्यन्त तद्वाक्यं यदुक्तं सव्यसाचिना\r"
  },
  {
    "id": "17001006a",
    "book": "17",
    "chapter": "001",
    "verse": "006",
    "quarter_verse": "a",
    "verse_text": "ततो युयुत्सुमानाय्य प्रव्रजन्धर्मकाम्यया\r"
  },
  {
    "id": "17001006c",
    "book": "17",
    "chapter": "001",
    "verse": "006",
    "quarter_verse": "c",
    "verse_text": "राज्यं परिददौ सर्वं वैश्यापुत्रे युधिष्ठिरः\r"
  },
  {
    "id": "17001007a",
    "book": "17",
    "chapter": "001",
    "verse": "007",
    "quarter_verse": "a",
    "verse_text": "अभिषिच्य स्वराज्ये तु तं राजानं परिक्षितम्\r"
  },
  {
    "id": "17001007c",
    "book": "17",
    "chapter": "001",
    "verse": "007",
    "quarter_verse": "c",
    "verse_text": "दुःखार्तश्चाब्रवीद्राजा सुभद्रां पाण्डवाग्रजः\r"
  },
  {
    "id": "17001008a",
    "book": "17",
    "chapter": "001",
    "verse": "008",
    "quarter_verse": "a",
    "verse_text": "एष पुत्रस्य ते पुत्रः कुरुराजो भविष्यति\r"
  },
  {
    "id": "17001008c",
    "book": "17",
    "chapter": "001",
    "verse": "008",
    "quarter_verse": "c",
    "verse_text": "यदूनां परिशेषश्च वज्रो राजा कृतश्च ह\r"
  },
  {
    "id": "17001009a",
    "book": "17",
    "chapter": "001",
    "verse": "009",
    "quarter_verse": "a",
    "verse_text": "परिक्षिद्धास्तिनपुरे शक्रप्रस्थे तु यादवः\r"
  },
  {
    "id": "17001009c",
    "book": "17",
    "chapter": "001",
    "verse": "009",
    "quarter_verse": "c",
    "verse_text": "वज्रो राजा त्वया रक्ष्यो मा चाधर्मे मनः कृथाः\r"
  },
  {
    "id": "17001010a",
    "book": "17",
    "chapter": "001",
    "verse": "010",
    "quarter_verse": "a",
    "verse_text": "इत्युक्त्वा धर्मराजः स वासुदेवस्य धीमतः\r"
  },
  {
    "id": "17001010c",
    "book": "17",
    "chapter": "001",
    "verse": "010",
    "quarter_verse": "c",
    "verse_text": "मातुलस्य च वृद्धस्य रामादीनां तथैव च\r"
  },
  {
    "id": "17001011a",
    "book": "17",
    "chapter": "001",
    "verse": "011",
    "quarter_verse": "a",
    "verse_text": "भ्रातृभिः सह धर्मात्मा कृत्वोदकमतन्द्रितः\r"
  },
  {
    "id": "17001011c",
    "book": "17",
    "chapter": "001",
    "verse": "011",
    "quarter_verse": "c",
    "verse_text": "श्राद्धान्युद्दिश्य सर्वेषां चकार विधिवत्तदा\r"
  },
  {
    "id": "17001012a",
    "book": "17",
    "chapter": "001",
    "verse": "012",
    "quarter_verse": "a",
    "verse_text": "ददौ रत्नानि वासांसि ग्रामानश्वान्रथानपि\r"
  },
  {
    "id": "17001012c",
    "book": "17",
    "chapter": "001",
    "verse": "012",
    "quarter_verse": "c",
    "verse_text": "स्त्रियश्च द्विजमुख्येभ्यो गवां शतसहस्रशः\r"
  },
  {
    "id": "17001013a",
    "book": "17",
    "chapter": "001",
    "verse": "013",
    "quarter_verse": "a",
    "verse_text": "कृपमभ्यर्च्य च गुरुमर्थमानपुरस्कृतम्\r"
  },
  {
    "id": "17001013c",
    "book": "17",
    "chapter": "001",
    "verse": "013",
    "quarter_verse": "c",
    "verse_text": "शिष्यं परिक्षितं तस्मै ददौ भरतसत्तमः\r"
  },
  {
    "id": "17001014a",
    "book": "17",
    "chapter": "001",
    "verse": "014",
    "quarter_verse": "a",
    "verse_text": "ततस्तु प्रकृतीः सर्वाः समानाय्य युधिष्ठिरः\r"
  },
  {
    "id": "17001014c",
    "book": "17",
    "chapter": "001",
    "verse": "014",
    "quarter_verse": "c",
    "verse_text": "सर्वमाचष्ट राजर्षिश्चिकीर्षितमथात्मनः\r"
  },
  {
    "id": "17001015a",
    "book": "17",
    "chapter": "001",
    "verse": "015",
    "quarter_verse": "a",
    "verse_text": "ते श्रुत्वैव वचस्तस्य पौरजानपदा जनाः\r"
  },
  {
    "id": "17001015c",
    "book": "17",
    "chapter": "001",
    "verse": "015",
    "quarter_verse": "c",
    "verse_text": "भृशमुद्विग्नमनसो नाभ्यनन्दन्त तद्वचः\r"
  },
  {
    "id": "17001016a",
    "book": "17",
    "chapter": "001",
    "verse": "016",
    "quarter_verse": "a",
    "verse_text": "नैवं कर्तव्यमिति ते तदोचुस्ते नराधिपम्\r"
  },
  {
    "id": "17001016c",
    "book": "17",
    "chapter": "001",
    "verse": "016",
    "quarter_verse": "c",
    "verse_text": "न च राजा तथाकार्षीत्कालपर्यायधर्मवित्\r"
  },
  {
    "id": "17001017a",
    "book": "17",
    "chapter": "001",
    "verse": "017",
    "quarter_verse": "a",
    "verse_text": "ततोऽनुमान्य धर्मात्मा पौरजानपदं जनम्\r"
  },
  {
    "id": "17001017c",
    "book": "17",
    "chapter": "001",
    "verse": "017",
    "quarter_verse": "c",
    "verse_text": "गमनाय मतिं चक्रे भ्रातरश्चास्य ते तदा\r"
  },
  {
    "id": "17001018a",
    "book": "17",
    "chapter": "001",
    "verse": "018",
    "quarter_verse": "a",
    "verse_text": "ततः स राजा कौरव्यो धर्मपुत्रो युधिष्ठिरः\r"
  },
  {
    "id": "17001018c",
    "book": "17",
    "chapter": "001",
    "verse": "018",
    "quarter_verse": "c",
    "verse_text": "उत्सृज्याभरणान्यङ्गाज्जगृहे वल्कलान्युत\r"
  },
  {
    "id": "17001019a",
    "book": "17",
    "chapter": "001",
    "verse": "019",
    "quarter_verse": "a",
    "verse_text": "भीमार्जुनौ यमौ चैव द्रौपदी च यशस्विनी\r"
  },
  {
    "id": "17001019c",
    "book": "17",
    "chapter": "001",
    "verse": "019",
    "quarter_verse": "c",
    "verse_text": "तथैव सर्वे जगृहुर्वल्कलानि जनाधिप\r"
  },
  {
    "id": "17001020a",
    "book": "17",
    "chapter": "001",
    "verse": "020",
    "quarter_verse": "a",
    "verse_text": "विधिवत्कारयित्वेष्टिं नैष्ठिकीं भरतर्षभ\r"
  },
  {
    "id": "17001020c",
    "book": "17",
    "chapter": "001",
    "verse": "020",
    "quarter_verse": "c",
    "verse_text": "समुत्सृज्याप्सु सर्वेऽग्नीन्प्रतस्थुर्नरपुंगवाः\r"
  },
  {
    "id": "17001021a",
    "book": "17",
    "chapter": "001",
    "verse": "021",
    "quarter_verse": "a",
    "verse_text": "ततः प्ररुरुदुः सर्वाः स्त्रियो दृष्ट्वा नरर्षभान्\r"
  },
  {
    "id": "17001021c",
    "book": "17",
    "chapter": "001",
    "verse": "021",
    "quarter_verse": "c",
    "verse_text": "प्रस्थितान्द्रौपदीषष्ठान्पुरा द्यूतजितान्यथा\r"
  },
  {
    "id": "17001022a",
    "book": "17",
    "chapter": "001",
    "verse": "022",
    "quarter_verse": "a",
    "verse_text": "हर्षोऽभवच्च सर्वेषां भ्रातॄणां गमनं प्रति\r"
  },
  {
    "id": "17001022c",
    "book": "17",
    "chapter": "001",
    "verse": "022",
    "quarter_verse": "c",
    "verse_text": "युधिष्ठिरमतं ज्ञात्वा वृष्णिक्षयमवेक्ष्य च\r"
  },
  {
    "id": "17001023a",
    "book": "17",
    "chapter": "001",
    "verse": "023",
    "quarter_verse": "a",
    "verse_text": "भ्रातरः पञ्च कृष्णा च षष्ठी श्वा चैव सप्तमः\r"
  },
  {
    "id": "17001023c",
    "book": "17",
    "chapter": "001",
    "verse": "023",
    "quarter_verse": "c",
    "verse_text": "आत्मना सप्तमो राजा निर्ययौ गजसाह्वयात्\r"
  },
  {
    "id": "17001023e",
    "book": "17",
    "chapter": "001",
    "verse": "023",
    "quarter_verse": "e",
    "verse_text": "पौरैरनुगतो दूरं सर्वैरन्तःपुरैस्तथा\r"
  },
  {
    "id": "17001024a",
    "book": "17",
    "chapter": "001",
    "verse": "024",
    "quarter_verse": "a",
    "verse_text": "न चैनमशकत्कश्चिन्निवर्तस्वेति भाषितुम्\r"
  },
  {
    "id": "17001024c",
    "book": "17",
    "chapter": "001",
    "verse": "024",
    "quarter_verse": "c",
    "verse_text": "न्यवर्तन्त ततः सर्वे नरा नगरवासिनः\r"
  },
  {
    "id": "17001025a",
    "book": "17",
    "chapter": "001",
    "verse": "025",
    "quarter_verse": "a",
    "verse_text": "कृपप्रभृतयश्चैव युयुत्सुं पर्यवारयन्\r"
  },
  {
    "id": "17001025c",
    "book": "17",
    "chapter": "001",
    "verse": "025",
    "quarter_verse": "c",
    "verse_text": "विवेश गङ्गां कौरव्य उलूपी भुजगात्मजा\r"
  },
  {
    "id": "17001026a",
    "book": "17",
    "chapter": "001",
    "verse": "026",
    "quarter_verse": "a",
    "verse_text": "चित्राङ्गदा ययौ चापि मणिपूरपुरं प्रति\r"
  },
  {
    "id": "17001026c",
    "book": "17",
    "chapter": "001",
    "verse": "026",
    "quarter_verse": "c",
    "verse_text": "शिष्टाः परिक्षितं त्वन्या मातरः पर्यवारयन्\r"
  },
  {
    "id": "17001027a",
    "book": "17",
    "chapter": "001",
    "verse": "027",
    "quarter_verse": "a",
    "verse_text": "पाण्डवाश्च महात्मानो द्रौपदी च यशस्विनी\r"
  },
  {
    "id": "17001027c",
    "book": "17",
    "chapter": "001",
    "verse": "027",
    "quarter_verse": "c",
    "verse_text": "कृतोपवासाः कौरव्य प्रययुः प्राङ्मुखास्ततः\r"
  },
  {
    "id": "17001028a",
    "book": "17",
    "chapter": "001",
    "verse": "028",
    "quarter_verse": "a",
    "verse_text": "योगयुक्ता महात्मानस्त्यागधर्ममुपेयुषः\r"
  },
  {
    "id": "17001028c",
    "book": "17",
    "chapter": "001",
    "verse": "028",
    "quarter_verse": "c",
    "verse_text": "अभिजग्मुर्बहून्देशान्सरितः पर्वतांस्तथा\r"
  },
  {
    "id": "17001029a",
    "book": "17",
    "chapter": "001",
    "verse": "029",
    "quarter_verse": "a",
    "verse_text": "युधिष्ठिरो ययावग्रे भीमस्तु तदनन्तरम्\r"
  },
  {
    "id": "17001029c",
    "book": "17",
    "chapter": "001",
    "verse": "029",
    "quarter_verse": "c",
    "verse_text": "अर्जुनस्तस्य चान्वेव यमौ चैव यथाक्रमम्\r"
  },
  {
    "id": "17001030a",
    "book": "17",
    "chapter": "001",
    "verse": "030",
    "quarter_verse": "a",
    "verse_text": "पृष्ठतस्तु वरारोहा श्यामा पद्मदलेक्षणा\r"
  },
  {
    "id": "17001030c",
    "book": "17",
    "chapter": "001",
    "verse": "030",
    "quarter_verse": "c",
    "verse_text": "द्रौपदी योषितां श्रेष्ठा ययौ भरतसत्तम\r"
  },
  {
    "id": "17001031a",
    "book": "17",
    "chapter": "001",
    "verse": "031",
    "quarter_verse": "a",
    "verse_text": "श्वा चैवानुययावेकः पाण्डवान्प्रस्थितान्वने\r"
  },
  {
    "id": "17001031c",
    "book": "17",
    "chapter": "001",
    "verse": "031",
    "quarter_verse": "c",
    "verse_text": "क्रमेण ते ययुर्वीरा लौहित्यं सलिलार्णवम्\r"
  },
  {
    "id": "17001032a",
    "book": "17",
    "chapter": "001",
    "verse": "032",
    "quarter_verse": "a",
    "verse_text": "गाण्डीवं च धनुर्दिव्यं न मुमोच धनंजयः\r"
  },
  {
    "id": "17001032c",
    "book": "17",
    "chapter": "001",
    "verse": "032",
    "quarter_verse": "c",
    "verse_text": "रत्नलोभान्महाराज तौ चाक्षय्यौ महेषुधी\r"
  },
  {
    "id": "17001033a",
    "book": "17",
    "chapter": "001",
    "verse": "033",
    "quarter_verse": "a",
    "verse_text": "अग्निं ते ददृशुस्तत्र स्थितं शैलमिवाग्रतः\r"
  },
  {
    "id": "17001033c",
    "book": "17",
    "chapter": "001",
    "verse": "033",
    "quarter_verse": "c",
    "verse_text": "मार्गमावृत्य तिष्ठन्तं साक्षात्पुरुषविग्रहम्\r"
  },
  {
    "id": "17001034a",
    "book": "17",
    "chapter": "001",
    "verse": "034",
    "quarter_verse": "a",
    "verse_text": "ततो देवः स सप्तार्चिः पाण्डवानिदमब्रवीत्\r"
  },
  {
    "id": "17001034c",
    "book": "17",
    "chapter": "001",
    "verse": "034",
    "quarter_verse": "c",
    "verse_text": "भो भो पाण्डुसुता वीराः पावकं मां विबोधत\r"
  },
  {
    "id": "17001035a",
    "book": "17",
    "chapter": "001",
    "verse": "035",
    "quarter_verse": "a",
    "verse_text": "युधिष्ठिर महाबाहो भीमसेन परंतप\r"
  },
  {
    "id": "17001035c",
    "book": "17",
    "chapter": "001",
    "verse": "035",
    "quarter_verse": "c",
    "verse_text": "अर्जुनाश्विसुतौ वीरौ निबोधत वचो मम\r"
  },
  {
    "id": "17001036a",
    "book": "17",
    "chapter": "001",
    "verse": "036",
    "quarter_verse": "a",
    "verse_text": "अहमग्निः कुरुश्रेष्ठा मया दग्धं च खाण्डवम्\r"
  },
  {
    "id": "17001036c",
    "book": "17",
    "chapter": "001",
    "verse": "036",
    "quarter_verse": "c",
    "verse_text": "अर्जुनस्य प्रभावेण तथा नारायणस्य च\r"
  },
  {
    "id": "17001037a",
    "book": "17",
    "chapter": "001",
    "verse": "037",
    "quarter_verse": "a",
    "verse_text": "अयं वः फल्गुनो भ्राता गाण्डीवं परमायुधम्\r"
  },
  {
    "id": "17001037c",
    "book": "17",
    "chapter": "001",
    "verse": "037",
    "quarter_verse": "c",
    "verse_text": "परित्यज्य वनं यातु नानेनार्थोऽस्ति कश्चन\r"
  },
  {
    "id": "17001038a",
    "book": "17",
    "chapter": "001",
    "verse": "038",
    "quarter_verse": "a",
    "verse_text": "चक्ररत्नं तु यत्कृष्णे स्थितमासीन्महात्मनि\r"
  },
  {
    "id": "17001038c",
    "book": "17",
    "chapter": "001",
    "verse": "038",
    "quarter_verse": "c",
    "verse_text": "गतं तच्च पुनर्हस्ते कालेनैष्यति तस्य ह\r"
  },
  {
    "id": "17001039a",
    "book": "17",
    "chapter": "001",
    "verse": "039",
    "quarter_verse": "a",
    "verse_text": "वरुणादाहृतं पूर्वं मयैतत्पार्थकारणात्\r"
  },
  {
    "id": "17001039c",
    "book": "17",
    "chapter": "001",
    "verse": "039",
    "quarter_verse": "c",
    "verse_text": "गाण्डीवं कार्मुकश्रेष्ठं वरुणायैव दीयताम्\r"
  },
  {
    "id": "17001040a",
    "book": "17",
    "chapter": "001",
    "verse": "040",
    "quarter_verse": "a",
    "verse_text": "ततस्ते भ्रातरः सर्वे धनंजयमचोदयन्\r"
  },
  {
    "id": "17001040c",
    "book": "17",
    "chapter": "001",
    "verse": "040",
    "quarter_verse": "c",
    "verse_text": "स जले प्राक्षिपत्तत्तु तथाक्षय्यौ महेषुधी\r"
  },
  {
    "id": "17001041a",
    "book": "17",
    "chapter": "001",
    "verse": "041",
    "quarter_verse": "a",
    "verse_text": "ततोऽग्निर्भरतश्रेष्ठ तत्रैवान्तरधीयत\r"
  },
  {
    "id": "17001041c",
    "book": "17",
    "chapter": "001",
    "verse": "041",
    "quarter_verse": "c",
    "verse_text": "ययुश्च पाण्डवा वीरास्ततस्ते दक्षिणामुखाः\r"
  },
  {
    "id": "17001042a",
    "book": "17",
    "chapter": "001",
    "verse": "042",
    "quarter_verse": "a",
    "verse_text": "ततस्ते तूत्तरेणैव तीरेण लवणाम्भसः\r"
  },
  {
    "id": "17001042c",
    "book": "17",
    "chapter": "001",
    "verse": "042",
    "quarter_verse": "c",
    "verse_text": "जग्मुर्भरतशार्दूल दिशं दक्षिणपश्चिमम्\r"
  },
  {
    "id": "17001043a",
    "book": "17",
    "chapter": "001",
    "verse": "043",
    "quarter_verse": "a",
    "verse_text": "ततः पुनः समावृत्ताः पश्चिमां दिशमेव ते\r"
  },
  {
    "id": "17001043c",
    "book": "17",
    "chapter": "001",
    "verse": "043",
    "quarter_verse": "c",
    "verse_text": "ददृशुर्द्वारकां चापि सागरेण परिप्लुताम्\r"
  },
  {
    "id": "17001044a",
    "book": "17",
    "chapter": "001",
    "verse": "044",
    "quarter_verse": "a",
    "verse_text": "उदीचीं पुनरावृत्त्य ययुर्भरतसत्तमाः\r"
  },
  {
    "id": "17001044c",
    "book": "17",
    "chapter": "001",
    "verse": "044",
    "quarter_verse": "c",
    "verse_text": "प्रादक्षिण्यं चिकीर्षन्तः पृथिव्या योगधर्मिणः\r"
  },
  {
    "id": "17002001",
    "book": "17",
    "chapter": "002",
    "verse": "001",
    "verse_text": " वैशंपायन उवाच\r"
  },
  {
    "id": "17002001a",
    "book": "17",
    "chapter": "002",
    "verse": "001",
    "quarter_verse": "a",
    "verse_text": "ततस्ते नियतात्मान उदीचीं दिशमास्थिताः\r"
  },
  {
    "id": "17002001c",
    "book": "17",
    "chapter": "002",
    "verse": "001",
    "quarter_verse": "c",
    "verse_text": "ददृशुर्योगयुक्ताश्च हिमवन्तं महागिरिम्\r"
  },
  {
    "id": "17002002a",
    "book": "17",
    "chapter": "002",
    "verse": "002",
    "quarter_verse": "a",
    "verse_text": "तं चाप्यतिक्रमन्तस्ते ददृशुर्वालुकार्णवम्\r"
  },
  {
    "id": "17002002c",
    "book": "17",
    "chapter": "002",
    "verse": "002",
    "quarter_verse": "c",
    "verse_text": "अवैक्षन्त महाशैलं मेरुं शिखरिणां वरम्\r"
  },
  {
    "id": "17002003a",
    "book": "17",
    "chapter": "002",
    "verse": "003",
    "quarter_verse": "a",
    "verse_text": "तेषां तु गच्छतां शीघ्रं सर्वेषां योगधर्मिणाम्\r"
  },
  {
    "id": "17002003c",
    "book": "17",
    "chapter": "002",
    "verse": "003",
    "quarter_verse": "c",
    "verse_text": "याज्ञसेनी भ्रष्टयोगा निपपात महीतले\r"
  },
  {
    "id": "17002004a",
    "book": "17",
    "chapter": "002",
    "verse": "004",
    "quarter_verse": "a",
    "verse_text": "तां तु प्रपतितां दृष्ट्वा भीमसेनो महाबलः\r"
  },
  {
    "id": "17002004c",
    "book": "17",
    "chapter": "002",
    "verse": "004",
    "quarter_verse": "c",
    "verse_text": "उवाच धर्मराजानं याज्ञसेनीमवेक्ष्य ह\r"
  },
  {
    "id": "17002005a",
    "book": "17",
    "chapter": "002",
    "verse": "005",
    "quarter_verse": "a",
    "verse_text": "नाधर्मश्चरितः कश्चिद्राजपुत्र्या परंतप\r"
  },
  {
    "id": "17002005c",
    "book": "17",
    "chapter": "002",
    "verse": "005",
    "quarter_verse": "c",
    "verse_text": "कारणं किं नु तद्राजन्यत्कृष्णा पतिता भुवि\r"
  },
  {
    "id": "17002006",
    "book": "17",
    "chapter": "002",
    "verse": "006",
    "verse_text": " युधिष्ठिर उवाच\r"
  },
  {
    "id": "17002006a",
    "book": "17",
    "chapter": "002",
    "verse": "006",
    "quarter_verse": "a",
    "verse_text": "पक्षपातो महानस्या विशेषेण धनंजये\r"
  },
  {
    "id": "17002006c",
    "book": "17",
    "chapter": "002",
    "verse": "006",
    "quarter_verse": "c",
    "verse_text": "तस्यैतत्फलमद्यैषा भुङ्क्ते पुरुषसत्तम\r"
  },
  {
    "id": "17002007",
    "book": "17",
    "chapter": "002",
    "verse": "007",
    "verse_text": " वैशंपायन उवाच\r"
  },
  {
    "id": "17002007a",
    "book": "17",
    "chapter": "002",
    "verse": "007",
    "quarter_verse": "a",
    "verse_text": "एवमुक्त्वानवेक्ष्यैनां ययौ धर्मसुतो नृपः\r"
  },
  {
    "id": "17002007c",
    "book": "17",
    "chapter": "002",
    "verse": "007",
    "quarter_verse": "c",
    "verse_text": "समाधाय मनो धीमान्धर्मात्मा पुरुषर्षभः\r"
  },
  {
    "id": "17002008a",
    "book": "17",
    "chapter": "002",
    "verse": "008",
    "quarter_verse": "a",
    "verse_text": "सहदेवस्ततो धीमान्निपपात महीतले\r"
  },
  {
    "id": "17002008c",
    "book": "17",
    "chapter": "002",
    "verse": "008",
    "quarter_verse": "c",
    "verse_text": "तं चापि पतितं दृष्ट्वा भीमो राजानमब्रवीत्\r"
  },
  {
    "id": "17002009a",
    "book": "17",
    "chapter": "002",
    "verse": "009",
    "quarter_verse": "a",
    "verse_text": "योऽयमस्मासु सर्वेषु शुश्रूषुरनहंकृतः\r"
  },
  {
    "id": "17002009c",
    "book": "17",
    "chapter": "002",
    "verse": "009",
    "quarter_verse": "c",
    "verse_text": "सोऽयं माद्रवतीपुत्रः कस्मान्निपतितो भुवि\r"
  },
  {
    "id": "17002010",
    "book": "17",
    "chapter": "002",
    "verse": "010",
    "verse_text": " युधिष्ठिर उवाच\r"
  },
  {
    "id": "17002010a",
    "book": "17",
    "chapter": "002",
    "verse": "010",
    "quarter_verse": "a",
    "verse_text": "आत्मनः सदृशं प्राज्ञं नैषोऽमन्यत कंचन\r"
  },
  {
    "id": "17002010c",
    "book": "17",
    "chapter": "002",
    "verse": "010",
    "quarter_verse": "c",
    "verse_text": "तेन दोषेण पतितस्तस्मादेष नृपात्मजः\r"
  },
  {
    "id": "17002011",
    "book": "17",
    "chapter": "002",
    "verse": "011",
    "verse_text": " वैशंपायन उवाच\r"
  },
  {
    "id": "17002011a",
    "book": "17",
    "chapter": "002",
    "verse": "011",
    "quarter_verse": "a",
    "verse_text": "इत्युक्त्वा तु समुत्सृज्य सहदेवं ययौ तदा\r"
  },
  {
    "id": "17002011c",
    "book": "17",
    "chapter": "002",
    "verse": "011",
    "quarter_verse": "c",
    "verse_text": "भ्रातृभिः सह कौन्तेयः शुना चैव युधिष्ठिरः\r"
  },
  {
    "id": "17002012a",
    "book": "17",
    "chapter": "002",
    "verse": "012",
    "quarter_verse": "a",
    "verse_text": "कृष्णां निपतितां दृष्ट्वा सहदेवं च पाण्डवम्\r"
  },
  {
    "id": "17002012c",
    "book": "17",
    "chapter": "002",
    "verse": "012",
    "quarter_verse": "c",
    "verse_text": "आर्तो बन्धुप्रियः शूरो नकुलो निपपात ह\r"
  },
  {
    "id": "17002013a",
    "book": "17",
    "chapter": "002",
    "verse": "013",
    "quarter_verse": "a",
    "verse_text": "तस्मिन्निपतिते वीरे नकुले चारुदर्शने\r"
  },
  {
    "id": "17002013c",
    "book": "17",
    "chapter": "002",
    "verse": "013",
    "quarter_verse": "c",
    "verse_text": "पुनरेव तदा भीमो राजानमिदमब्रवीत्\r"
  },
  {
    "id": "17002014a",
    "book": "17",
    "chapter": "002",
    "verse": "014",
    "quarter_verse": "a",
    "verse_text": "योऽयमक्षतधर्मात्मा भ्राता वचनकारकः\r"
  },
  {
    "id": "17002014c",
    "book": "17",
    "chapter": "002",
    "verse": "014",
    "quarter_verse": "c",
    "verse_text": "रूपेणाप्रतिमो लोके नकुलः पतितो भुवि\r"
  },
  {
    "id": "17002015a",
    "book": "17",
    "chapter": "002",
    "verse": "015",
    "quarter_verse": "a",
    "verse_text": "इत्युक्तो भीमसेनेन प्रत्युवाच युधिष्ठिरः\r"
  },
  {
    "id": "17002015c",
    "book": "17",
    "chapter": "002",
    "verse": "015",
    "quarter_verse": "c",
    "verse_text": "नकुलं प्रति धर्मात्मा सर्वबुद्धिमतां वरः\r"
  },
  {
    "id": "17002016a",
    "book": "17",
    "chapter": "002",
    "verse": "016",
    "quarter_verse": "a",
    "verse_text": "रूपेण मत्समो नास्ति कश्चिदित्यस्य दर्शनम्\r"
  },
  {
    "id": "17002016c",
    "book": "17",
    "chapter": "002",
    "verse": "016",
    "quarter_verse": "c",
    "verse_text": "अधिकश्चाहमेवैक इत्यस्य मनसि स्थितम्\r"
  },
  {
    "id": "17002017a",
    "book": "17",
    "chapter": "002",
    "verse": "017",
    "quarter_verse": "a",
    "verse_text": "नकुलः पतितस्तस्मादागच्छ त्वं वृकोदर\r"
  },
  {
    "id": "17002017c",
    "book": "17",
    "chapter": "002",
    "verse": "017",
    "quarter_verse": "c",
    "verse_text": "यस्य यद्विहितं वीर सोऽवश्यं तदुपाश्नुते\r"
  },
  {
    "id": "17002018a",
    "book": "17",
    "chapter": "002",
    "verse": "018",
    "quarter_verse": "a",
    "verse_text": "तांस्तु प्रपतितान्दृष्ट्वा पाण्डवः श्वेतवाहनः\r"
  },
  {
    "id": "17002018c",
    "book": "17",
    "chapter": "002",
    "verse": "018",
    "quarter_verse": "c",
    "verse_text": "पपात शोकसंतप्तस्ततोऽनु परवीरहा\r"
  },
  {
    "id": "17002019a",
    "book": "17",
    "chapter": "002",
    "verse": "019",
    "quarter_verse": "a",
    "verse_text": "तस्मिंस्तु पुरुषव्याघ्रे पतिते शक्रतेजसि\r"
  },
  {
    "id": "17002019c",
    "book": "17",
    "chapter": "002",
    "verse": "019",
    "quarter_verse": "c",
    "verse_text": "म्रियमाणे दुराधर्षे भीमो राजानमब्रवीत्\r"
  },
  {
    "id": "17002020a",
    "book": "17",
    "chapter": "002",
    "verse": "020",
    "quarter_verse": "a",
    "verse_text": "अनृतं न स्मराम्यस्य स्वैरेष्वपि महात्मनः\r"
  },
  {
    "id": "17002020c",
    "book": "17",
    "chapter": "002",
    "verse": "020",
    "quarter_verse": "c",
    "verse_text": "अथ कस्य विकारोऽयं येनायं पतितो भुवि\r"
  },
  {
    "id": "17002021",
    "book": "17",
    "chapter": "002",
    "verse": "021",
    "verse_text": " युधिष्ठिर उवाच\r"
  },
  {
    "id": "17002021a",
    "book": "17",
    "chapter": "002",
    "verse": "021",
    "quarter_verse": "a",
    "verse_text": "एकाह्ना निर्दहेयं वै शत्रूनित्यर्जुनोऽब्रवीत्\r"
  },
  {
    "id": "17002021c",
    "book": "17",
    "chapter": "002",
    "verse": "021",
    "quarter_verse": "c",
    "verse_text": "न च तत्कृतवानेष शूरमानी ततोऽपतत्\r"
  },
  {
    "id": "17002022a",
    "book": "17",
    "chapter": "002",
    "verse": "022",
    "quarter_verse": "a",
    "verse_text": "अवमेने धनुर्ग्राहानेष सर्वांश्च फल्गुनः\r"
  },
  {
    "id": "17002022c",
    "book": "17",
    "chapter": "002",
    "verse": "022",
    "quarter_verse": "c",
    "verse_text": "यथा चोक्तं तथा चैव कर्तव्यं भूतिमिच्छता\r"
  },
  {
    "id": "17002023",
    "book": "17",
    "chapter": "002",
    "verse": "023",
    "verse_text": " वैशंपायन उवाच\r"
  },
  {
    "id": "17002023a",
    "book": "17",
    "chapter": "002",
    "verse": "023",
    "quarter_verse": "a",
    "verse_text": "इत्युक्त्वा प्रस्थितो राजा भीमोऽथ निपपात ह\r"
  },
  {
    "id": "17002023c",
    "book": "17",
    "chapter": "002",
    "verse": "023",
    "quarter_verse": "c",
    "verse_text": "पतितश्चाब्रवीद्भीमो धर्मराजं युधिष्ठिरम्\r"
  },
  {
    "id": "17002024a",
    "book": "17",
    "chapter": "002",
    "verse": "024",
    "quarter_verse": "a",
    "verse_text": "भो भो राजन्नवेक्षस्व पतितोऽहं प्रियस्तव\r"
  },
  {
    "id": "17002024c",
    "book": "17",
    "chapter": "002",
    "verse": "024",
    "quarter_verse": "c",
    "verse_text": "किंनिमित्तं च पतनं ब्रूहि मे यदि वेत्थ ह\r"
  },
  {
    "id": "17002025",
    "book": "17",
    "chapter": "002",
    "verse": "025",
    "verse_text": " युधिष्ठिर उवाच\r"
  },
  {
    "id": "17002025a",
    "book": "17",
    "chapter": "002",
    "verse": "025",
    "quarter_verse": "a",
    "verse_text": "अतिभुक्तं च भवता प्राणेन च विकत्थसे\r"
  },
  {
    "id": "17002025c",
    "book": "17",
    "chapter": "002",
    "verse": "025",
    "quarter_verse": "c",
    "verse_text": "अनवेक्ष्य परं पार्थ तेनासि पतितः क्षितौ\r"
  },
  {
    "id": "17002026",
    "book": "17",
    "chapter": "002",
    "verse": "026",
    "verse_text": " वैशंपायन उवाच\r"
  },
  {
    "id": "17002026a",
    "book": "17",
    "chapter": "002",
    "verse": "026",
    "quarter_verse": "a",
    "verse_text": "इत्युक्त्वा तं महाबाहुर्जगामानवलोकयन्\r"
  },
  {
    "id": "17002026c",
    "book": "17",
    "chapter": "002",
    "verse": "026",
    "quarter_verse": "c",
    "verse_text": "श्वा त्वेकोऽनुययौ यस्ते बहुशः कीर्तितो मया\r"
  },
  {
    "id": "17003001",
    "book": "17",
    "chapter": "003",
    "verse": "001",
    "verse_text": " वैशंपायन उवाच\r"
  },
  {
    "id": "17003001a",
    "book": "17",
    "chapter": "003",
    "verse": "001",
    "quarter_verse": "a",
    "verse_text": "ततः संनादयञ्शक्रो दिवं भूमिं च सर्वशः\r"
  },
  {
    "id": "17003001c",
    "book": "17",
    "chapter": "003",
    "verse": "001",
    "quarter_verse": "c",
    "verse_text": "रथेनोपययौ पार्थमारोहेत्यब्रवीच्च तम्\r"
  },
  {
    "id": "17003002a",
    "book": "17",
    "chapter": "003",
    "verse": "002",
    "quarter_verse": "a",
    "verse_text": "स भ्रातॄन्पतितान्दृष्ट्वा धर्मराजो युधिष्ठिरः\r"
  },
  {
    "id": "17003002c",
    "book": "17",
    "chapter": "003",
    "verse": "002",
    "quarter_verse": "c",
    "verse_text": "अब्रवीच्छोकसंतप्तः सहस्राक्षमिदं वचः\r"
  },
  {
    "id": "17003003a",
    "book": "17",
    "chapter": "003",
    "verse": "003",
    "quarter_verse": "a",
    "verse_text": "भ्रातरः पतिता मेऽत्र आगच्छेयुर्मया सह\r"
  },
  {
    "id": "17003003c",
    "book": "17",
    "chapter": "003",
    "verse": "003",
    "quarter_verse": "c",
    "verse_text": "न विना भ्रातृभिः स्वर्गमिच्छे गन्तुं सुरेश्वर\r"
  },
  {
    "id": "17003004a",
    "book": "17",
    "chapter": "003",
    "verse": "004",
    "quarter_verse": "a",
    "verse_text": "सुकुमारी सुखार्हा च राजपुत्री पुरंदर\r"
  },
  {
    "id": "17003004c",
    "book": "17",
    "chapter": "003",
    "verse": "004",
    "quarter_verse": "c",
    "verse_text": "सास्माभिः सह गच्छेत तद्भवाननुमन्यताम्\r"
  },
  {
    "id": "17003005",
    "book": "17",
    "chapter": "003",
    "verse": "005",
    "verse_text": " इन्द्र उवाच\r"
  },
  {
    "id": "17003005a",
    "book": "17",
    "chapter": "003",
    "verse": "005",
    "quarter_verse": "a",
    "verse_text": "भ्रातॄन्द्रक्ष्यसि पुत्रांस्त्वमग्रतस्त्रिदिवं गतान्\r"
  },
  {
    "id": "17003005c",
    "book": "17",
    "chapter": "003",
    "verse": "005",
    "quarter_verse": "c",
    "verse_text": "कृष्णया सहितान्सर्वान्मा शुचो भरतर्षभ\r"
  },
  {
    "id": "17003006a",
    "book": "17",
    "chapter": "003",
    "verse": "006",
    "quarter_verse": "a",
    "verse_text": "निक्षिप्य मानुषं देहं गतास्ते भरतर्षभ\r"
  },
  {
    "id": "17003006c",
    "book": "17",
    "chapter": "003",
    "verse": "006",
    "quarter_verse": "c",
    "verse_text": "अनेन त्वं शरीरेण स्वर्गं गन्ता न संशयः\r"
  },
  {
    "id": "17003007",
    "book": "17",
    "chapter": "003",
    "verse": "007",
    "verse_text": " युधिष्ठिर उवाच\r"
  },
  {
    "id": "17003007a",
    "book": "17",
    "chapter": "003",
    "verse": "007",
    "quarter_verse": "a",
    "verse_text": "अयं श्वा भूतभव्येश भक्तो मां नित्यमेव ह\r"
  },
  {
    "id": "17003007c",
    "book": "17",
    "chapter": "003",
    "verse": "007",
    "quarter_verse": "c",
    "verse_text": "स गच्छेत मया सार्धमानृशंस्या हि मे मतिः\r"
  },
  {
    "id": "17003008",
    "book": "17",
    "chapter": "003",
    "verse": "008",
    "verse_text": " इन्द्र उवाच\r"
  },
  {
    "id": "17003008a",
    "book": "17",
    "chapter": "003",
    "verse": "008",
    "quarter_verse": "a",
    "verse_text": "अमर्त्यत्वं मत्समत्वं च राज;ञ्श्रियं कृत्स्नां महतीं चैव कीर्तिम्\r"
  },
  {
    "id": "17003008c",
    "book": "17",
    "chapter": "003",
    "verse": "008",
    "quarter_verse": "c",
    "verse_text": "संप्राप्तोऽद्य स्वर्गसुखानि च त्वं; त्यज श्वानं नात्र नृशंसमस्ति\r"
  },
  {
    "id": "17003009",
    "book": "17",
    "chapter": "003",
    "verse": "009",
    "verse_text": " युधिष्ठिर उवाच\r"
  },
  {
    "id": "17003009a",
    "book": "17",
    "chapter": "003",
    "verse": "009",
    "quarter_verse": "a",
    "verse_text": "अनार्यमार्येण सहस्रनेत्र; शक्यं कर्तुं दुष्करमेतदार्य\r"
  },
  {
    "id": "17003009c",
    "book": "17",
    "chapter": "003",
    "verse": "009",
    "quarter_verse": "c",
    "verse_text": "मा मे श्रिया संगमनं तयास्तु; यस्याः कृते भक्तजनं त्यजेयम्\r"
  },
  {
    "id": "17003010",
    "book": "17",
    "chapter": "003",
    "verse": "010",
    "verse_text": " इन्द्र उवाच\r"
  },
  {
    "id": "17003010a",
    "book": "17",
    "chapter": "003",
    "verse": "010",
    "quarter_verse": "a",
    "verse_text": "स्वर्गे लोके श्ववतां नास्ति धिष्ण्य;मिष्टापूर्तं क्रोधवशा हरन्ति\r"
  },
  {
    "id": "17003010c",
    "book": "17",
    "chapter": "003",
    "verse": "010",
    "quarter_verse": "c",
    "verse_text": "ततो विचार्य क्रियतां धर्मराज; त्यज श्वानं नात्र नृशंसमस्ति\r"
  },
  {
    "id": "17003011",
    "book": "17",
    "chapter": "003",
    "verse": "011",
    "verse_text": " युधिष्ठिर उवाच\r"
  },
  {
    "id": "17003011a",
    "book": "17",
    "chapter": "003",
    "verse": "011",
    "quarter_verse": "a",
    "verse_text": "भक्तत्यागं प्राहुरत्यन्तपापं; तुल्यं लोके ब्रह्मवध्याकृतेन\r"
  },
  {
    "id": "17003011c",
    "book": "17",
    "chapter": "003",
    "verse": "011",
    "quarter_verse": "c",
    "verse_text": "तस्मान्नाहं जातु कथंचनाद्य; त्यक्ष्याम्येनं स्वसुखार्थी महेन्द्र\r"
  },
  {
    "id": "17003012",
    "book": "17",
    "chapter": "003",
    "verse": "012",
    "verse_text": " इन्द्र उवाच\r"
  },
  {
    "id": "17003012a",
    "book": "17",
    "chapter": "003",
    "verse": "012",
    "quarter_verse": "a",
    "verse_text": "शुना दृष्टं क्रोधवशा हरन्ति; यद्दत्तमिष्टं विवृतमथो हुतं च\r"
  },
  {
    "id": "17003012c",
    "book": "17",
    "chapter": "003",
    "verse": "012",
    "quarter_verse": "c",
    "verse_text": "तस्माच्छुनस्त्यागमिमं कुरुष्व; शुनस्त्यागात्प्राप्स्यसे देवलोकम्\r"
  },
  {
    "id": "17003013a",
    "book": "17",
    "chapter": "003",
    "verse": "013",
    "quarter_verse": "a",
    "verse_text": "त्यक्त्वा भ्रातॄन्दयितां चापि कृष्णां; प्राप्तो लोकः कर्मणा स्वेन वीर\r"
  },
  {
    "id": "17003013c",
    "book": "17",
    "chapter": "003",
    "verse": "013",
    "quarter_verse": "c",
    "verse_text": "श्वानं चैनं न त्यजसे कथं नु; त्यागं कृत्स्नं चास्थितो मुह्यसेऽद्य\r"
  },
  {
    "id": "17003014",
    "book": "17",
    "chapter": "003",
    "verse": "014",
    "verse_text": " युधिष्ठिर उवाच\r"
  },
  {
    "id": "17003014a",
    "book": "17",
    "chapter": "003",
    "verse": "014",
    "quarter_verse": "a",
    "verse_text": "न विद्यते संधिरथापि विग्रहो; मृतैर्मर्त्यैरिति लोकेषु निष्ठा\r"
  },
  {
    "id": "17003014c",
    "book": "17",
    "chapter": "003",
    "verse": "014",
    "quarter_verse": "c",
    "verse_text": "न ते मया जीवयितुं हि शक्या; तस्मात्त्यागस्तेषु कृतो न जीवताम्\r"
  },
  {
    "id": "17003015a",
    "book": "17",
    "chapter": "003",
    "verse": "015",
    "quarter_verse": "a",
    "verse_text": "प्रतिप्रदानं शरणागतस्य; स्त्रिया वधो ब्राह्मणस्वापहारः\r"
  },
  {
    "id": "17003015c",
    "book": "17",
    "chapter": "003",
    "verse": "015",
    "quarter_verse": "c",
    "verse_text": "मित्रद्रोहस्तानि चत्वारि शक्र; भक्तत्यागश्चैव समो मतो मे\r"
  },
  {
    "id": "17003016",
    "book": "17",
    "chapter": "003",
    "verse": "016",
    "verse_text": " वैशंपायन उवाच\r"
  },
  {
    "id": "17003016a",
    "book": "17",
    "chapter": "003",
    "verse": "016",
    "quarter_verse": "a",
    "verse_text": "तद्धर्मराजस्य वचो निशम्य; धर्मस्वरूपी भगवानुवाच\r"
  },
  {
    "id": "17003016c",
    "book": "17",
    "chapter": "003",
    "verse": "016",
    "quarter_verse": "c",
    "verse_text": "युधिष्ठिरं प्रीतियुक्तो नरेन्द्रं; श्लक्ष्णैर्वाक्यैः संस्तवसंप्रयुक्तैः\r"
  },
  {
    "id": "17003017a",
    "book": "17",
    "chapter": "003",
    "verse": "017",
    "quarter_verse": "a",
    "verse_text": "अभिजातोऽसि राजेन्द्र पितुर्वृत्तेन मेधया\r"
  },
  {
    "id": "17003017c",
    "book": "17",
    "chapter": "003",
    "verse": "017",
    "quarter_verse": "c",
    "verse_text": "अनुक्रोशेन चानेन सर्वभूतेषु भारत\r"
  },
  {
    "id": "17003018a",
    "book": "17",
    "chapter": "003",
    "verse": "018",
    "quarter_verse": "a",
    "verse_text": "पुरा द्वैतवने चासि मया पुत्र परीक्षितः\r"
  },
  {
    "id": "17003018c",
    "book": "17",
    "chapter": "003",
    "verse": "018",
    "quarter_verse": "c",
    "verse_text": "पानीयार्थे पराक्रान्ता यत्र ते भ्रातरो हताः\r"
  },
  {
    "id": "17003019a",
    "book": "17",
    "chapter": "003",
    "verse": "019",
    "quarter_verse": "a",
    "verse_text": "भीमार्जुनौ परित्यज्य यत्र त्वं भ्रातरावुभौ\r"
  },
  {
    "id": "17003019c",
    "book": "17",
    "chapter": "003",
    "verse": "019",
    "quarter_verse": "c",
    "verse_text": "मात्रोः साम्यमभीप्सन्वै नकुलं जीवमिच्छसि\r"
  },
  {
    "id": "17003020a",
    "book": "17",
    "chapter": "003",
    "verse": "020",
    "quarter_verse": "a",
    "verse_text": "अयं श्वा भक्त इत्येव त्यक्तो देवरथस्त्वया\r"
  },
  {
    "id": "17003020c",
    "book": "17",
    "chapter": "003",
    "verse": "020",
    "quarter_verse": "c",
    "verse_text": "तस्मात्स्वर्गे न ते तुल्यः कश्चिदस्ति नराधिप\r"
  },
  {
    "id": "17003021a",
    "book": "17",
    "chapter": "003",
    "verse": "021",
    "quarter_verse": "a",
    "verse_text": "अतस्तवाक्षया लोकाः स्वशरीरेण भारत\r"
  },
  {
    "id": "17003021c",
    "book": "17",
    "chapter": "003",
    "verse": "021",
    "quarter_verse": "c",
    "verse_text": "प्राप्तोऽसि भरतश्रेष्ठ दिव्यां गतिमनुत्तमाम्\r"
  },
  {
    "id": "17003022a",
    "book": "17",
    "chapter": "003",
    "verse": "022",
    "quarter_verse": "a",
    "verse_text": "ततो धर्मश्च शक्रश्च मरुतश्चाश्विनावपि\r"
  },
  {
    "id": "17003022c",
    "book": "17",
    "chapter": "003",
    "verse": "022",
    "quarter_verse": "c",
    "verse_text": "देवा देवर्षयश्चैव रथमारोप्य पाण्डवम्\r"
  },
  {
    "id": "17003023a",
    "book": "17",
    "chapter": "003",
    "verse": "023",
    "quarter_verse": "a",
    "verse_text": "प्रययुः स्वैर्विमानैस्ते सिद्धाः कामविहारिणः\r"
  },
  {
    "id": "17003023c",
    "book": "17",
    "chapter": "003",
    "verse": "023",
    "quarter_verse": "c",
    "verse_text": "सर्वे विरजसः पुण्याः पुण्यवाग्बुद्धिकर्मिणः\r"
  },
  {
    "id": "17003024a",
    "book": "17",
    "chapter": "003",
    "verse": "024",
    "quarter_verse": "a",
    "verse_text": "स तं रथं समास्थाय राजा कुरुकुलोद्वहः\r"
  },
  {
    "id": "17003024c",
    "book": "17",
    "chapter": "003",
    "verse": "024",
    "quarter_verse": "c",
    "verse_text": "ऊर्ध्वमाचक्रमे शीघ्रं तेजसावृत्य रोदसी\r"
  },
  {
    "id": "17003025a",
    "book": "17",
    "chapter": "003",
    "verse": "025",
    "quarter_verse": "a",
    "verse_text": "ततो देवनिकायस्थो नारदः सर्वलोकवित्\r"
  },
  {
    "id": "17003025c",
    "book": "17",
    "chapter": "003",
    "verse": "025",
    "quarter_verse": "c",
    "verse_text": "उवाचोच्चैस्तदा वाक्यं बृहद्वादी बृहत्तपाः\r"
  },
  {
    "id": "17003026a",
    "book": "17",
    "chapter": "003",
    "verse": "026",
    "quarter_verse": "a",
    "verse_text": "येऽपि राजर्षयः सर्वे ते चापि समुपस्थिताः\r"
  },
  {
    "id": "17003026c",
    "book": "17",
    "chapter": "003",
    "verse": "026",
    "quarter_verse": "c",
    "verse_text": "कीर्तिं प्रच्छाद्य तेषां वै कुरुराजोऽधितिष्ठति\r"
  },
  {
    "id": "17003027a",
    "book": "17",
    "chapter": "003",
    "verse": "027",
    "quarter_verse": "a",
    "verse_text": "लोकानावृत्य यशसा तेजसा वृत्तसंपदा\r"
  },
  {
    "id": "17003027c",
    "book": "17",
    "chapter": "003",
    "verse": "027",
    "quarter_verse": "c",
    "verse_text": "स्वशरीरेण संप्राप्तं नान्यं शुश्रुम पाण्डवात्\r"
  },
  {
    "id": "17003028a",
    "book": "17",
    "chapter": "003",
    "verse": "028",
    "quarter_verse": "a",
    "verse_text": "नारदस्य वचः श्रुत्वा राजा वचनमब्रवीत्\r"
  },
  {
    "id": "17003028c",
    "book": "17",
    "chapter": "003",
    "verse": "028",
    "quarter_verse": "c",
    "verse_text": "देवानामन्त्र्य धर्मात्मा स्वपक्षांश्चैव पार्थिवान्\r"
  },
  {
    "id": "17003029a",
    "book": "17",
    "chapter": "003",
    "verse": "029",
    "quarter_verse": "a",
    "verse_text": "शुभं वा यदि वा पापं भ्रातॄणां स्थानमद्य मे\r"
  },
  {
    "id": "17003029c",
    "book": "17",
    "chapter": "003",
    "verse": "029",
    "quarter_verse": "c",
    "verse_text": "तदेव प्राप्तुमिच्छामि लोकानन्यान्न कामये\r"
  },
  {
    "id": "17003030a",
    "book": "17",
    "chapter": "003",
    "verse": "030",
    "quarter_verse": "a",
    "verse_text": "राज्ञस्तु वचनं श्रुत्वा देवराजः पुरंदरः\r"
  },
  {
    "id": "17003030c",
    "book": "17",
    "chapter": "003",
    "verse": "030",
    "quarter_verse": "c",
    "verse_text": "आनृशंस्यसमायुक्तं प्रत्युवाच युधिष्ठिरम्\r"
  },
  {
    "id": "17003031a",
    "book": "17",
    "chapter": "003",
    "verse": "031",
    "quarter_verse": "a",
    "verse_text": "स्थानेऽस्मिन्वस राजेन्द्र कर्मभिर्निर्जिते शुभैः\r"
  },
  {
    "id": "17003031c",
    "book": "17",
    "chapter": "003",
    "verse": "031",
    "quarter_verse": "c",
    "verse_text": "किं त्वं मानुष्यकं स्नेहमद्यापि परिकर्षसि\r"
  },
  {
    "id": "17003032a",
    "book": "17",
    "chapter": "003",
    "verse": "032",
    "quarter_verse": "a",
    "verse_text": "सिद्धिं प्राप्तोऽसि परमां यथा नान्यः पुमान्क्वचित्\r"
  },
  {
    "id": "17003032c",
    "book": "17",
    "chapter": "003",
    "verse": "032",
    "quarter_verse": "c",
    "verse_text": "नैव ते भ्रातरः स्थानं संप्राप्ताः कुरुनन्दन\r"
  },
  {
    "id": "17003033a",
    "book": "17",
    "chapter": "003",
    "verse": "033",
    "quarter_verse": "a",
    "verse_text": "अद्यापि मानुषो भावः स्पृशते त्वां नराधिप\r"
  },
  {
    "id": "17003033c",
    "book": "17",
    "chapter": "003",
    "verse": "033",
    "quarter_verse": "c",
    "verse_text": "स्वर्गोऽयं पश्य देवर्षीन्सिद्धांश्च त्रिदिवालयान्\r"
  },
  {
    "id": "17003034a",
    "book": "17",
    "chapter": "003",
    "verse": "034",
    "quarter_verse": "a",
    "verse_text": "युधिष्ठिरस्तु देवेन्द्रमेवंवादिनमीश्वरम्\r"
  },
  {
    "id": "17003034c",
    "book": "17",
    "chapter": "003",
    "verse": "034",
    "quarter_verse": "c",
    "verse_text": "पुनरेवाब्रवीद्धीमानिदं वचनमर्थवत्\r"
  },
  {
    "id": "17003035a",
    "book": "17",
    "chapter": "003",
    "verse": "035",
    "quarter_verse": "a",
    "verse_text": "तैर्विना नोत्सहे वस्तुमिह दैत्यनिबर्हण\r"
  },
  {
    "id": "17003035c",
    "book": "17",
    "chapter": "003",
    "verse": "035",
    "quarter_verse": "c",
    "verse_text": "गन्तुमिच्छामि तत्राहं यत्र मे भ्रातरो गताः\r"
  },
  {
    "id": "17003036a",
    "book": "17",
    "chapter": "003",
    "verse": "036",
    "quarter_verse": "a",
    "verse_text": "यत्र सा बृहती श्यामा बुद्धिसत्त्वगुणान्विता\r"
  },
  {
    "id": "17003036c",
    "book": "17",
    "chapter": "003",
    "verse": "036",
    "quarter_verse": "c",
    "verse_text": "द्रौपदी योषितां श्रेष्ठा यत्र चैव प्रिया मम\r"
  }
]